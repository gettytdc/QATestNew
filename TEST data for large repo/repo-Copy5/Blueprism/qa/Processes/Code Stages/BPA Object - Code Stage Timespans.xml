  <stage stageid="a03c197a-5738-42a9-80f3-b27c81c20431" name="Code1" type="Code">
    <subsheetid>511f1d83-65f0-4fbb-9577-b2802ab3f2ff</subsheetid>
    <loginhibit />
    <narrative>
    </narrative>
    <displayx>15</displayx>
    <displayy>-60</displayy>
    <displaywidth>60</displaywidth>
    <displayheight>30</displayheight>
    <font family="Tahoma" size="10" style="Regular" color="000000" />
    <inputs>
      <input type="collection" name="Colin" expr="[Coll1]" />
      <input type="timespan" name="Tsin" expr="[Data1]" />
    </inputs>
    <outputs>
      <output type="collection" name="Colout" stage="Coll2" />
      <output type="timespan" name="Tsout" stage="Data2" />
      <output type="flag" name="InputsOk" stage="Inputs Ok?" />
    </outputs>
    <onsuccess>7ebeacf8-efa4-4eb0-a989-c9de7984f8a4</onsuccess>
    <code><![CDATA[InputsOk = True

If TsIn.Days <> 1 OrElse TsIn.Hours <> 2 OrElse TsIn.Minutes <> 3 OrElse TsIn.Seconds <> 4 Then
	InputsOk = False
End If

Dim cts As Timespan
cts = Colin.Rows(0)("Field1")
If cts.Days <> 5 OrElse cts.Hours <> 6 OrElse cts.Minutes <> 7 OrElse cts.Seconds <> 8 Then
	InputsOk = False
End If

Dim cd As Date
cd = Colin.Rows(0)("Field2")
If cd.Day <> 1 OrElse cd.Month <> 2 OrElse cd.Year <> 2003 Then
	InputsOk = False
End If

Dim cdatetime As DateTime
cdatetime = Colin.Rows(0)("Field3")
If cdatetime.Day <> 1 OrElse cdatetime.Month <> 2 OrElse cdatetime.Year <> 2003 OrElse cdatetime.Hour <> 1 OrElse cdatetime.Minute <> 5 OrElse cdatetime.Second <> 6 Then
	InputsOk = False
End If


Tsout = New Timespan(1,2,3,4)

Colout = New System.Data.DataTable()
Colout.Columns.Add("Field1", System.Type.GetType("System.TimeSpan"))
Colout.Columns.Add("Field2", System.Type.GetType("System.DateTime"))
Colout.Columns.Add("Field3", System.Type.GetType("System.DateTime"))
Dim row As System.Data.DataRow = Colout.NewRow
row("Field1") = New Timespan(5,6,7,8)
row("Field2") = cd
row("Field3") = cdatetime
Colout.Rows.Add(row)



]]></code>
  </stage>
