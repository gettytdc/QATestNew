    <code><![CDATA[
System.Data.DataTable MainTable = new System.Data.DataTable();

System.Data.DataTable InnerTable = new System.Data.DataTable();

MainTable.Columns.Add(new System.Data.DataColumn("MainTableCol1", typeof(System.Data.DataTable)));
MainTable.Columns.Add(new System.Data.DataColumn("MainTableCol2", typeof(string)));

InnerTable.Columns.Add("InnerTableCol1", typeof(string));
System.Data.DataRow row1 = InnerTable.Rows.Add();

row1["InnerTableCol1"] = "User1";

System.Data.DataRow row = MainTable.Rows.Add();
row["MainTableCol1"] = InnerTable;
row["MainTableCol2"] = "StringData";

Generated = MainTable;
]]></code>
