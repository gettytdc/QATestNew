    <font family="Tahoma" size="10" style="Regular" color="000000" />
    <inputs>
      <input type="flag" name="Enabled" expr="[Enable ThoroughJAB]" />
    </inputs>
    <onsuccess>08b75530-bd23-46c2-9261-a29571bc1fb6</onsuccess>
    <code><![CDATA[Dim T as Type = GetType(BluePrism.ApplicationManager.ApplicationManagerUtilities.clsConfig)
Dim FI As FieldInfo = T.GetField("mbUseThoroughJABMatching", BindingFlags.NonPublic Or BindingFlags.Static)

If FI IsNot Nothing Then
	FI.SetValue(Nothing, Enabled)
Else
	Throw New ApplicationException("Config not found")
End If]]></code>
