  <stage stageid="1d9308c3-2d7e-4fde-9466-c0797ceed586" name="Hash Password" type="Code">
    <subsheetid>71a6d94d-798f-43d3-b5e6-752935dacf35</subsheetid>
    <loginhibit />
    <narrative>
    </narrative>
    <displayx>15</displayx>
    <displayy>75</displayy>
    <displaywidth>90</displaywidth>
    <displayheight>30</displayheight>
    <font family="Segoe UI" size="10" style="Regular" color="000000" />
    <inputs>
      <input type="text" name="Password" expr="[Password]" />
    </inputs>
    <outputs>
      <output type="text" name="Salt" stage="Salt" />
      <output type="text" name="Hash" stage="Hash" />
    </outputs>
    <onsuccess>1c951417-6637-4621-9c0a-ce24a1447c2a</onsuccess>
    <code><![CDATA[var saltBytes = new byte[32];

using(var cryptoProvider = new RNGCryptoServiceProvider())
{
	cryptoProvider.GetBytes(saltBytes);
}

byte[] passwordBytes;

using (var derivedBytes = new Rfc2898DeriveBytes(Encoding.UTF8.GetBytes(Password), saltBytes, 64000))
{
	passwordBytes = derivedBytes.GetBytes(32);
}

Hash = Convert.ToBase64String(passwordBytes);
Salt = Convert.ToBase64String(saltBytes);]]></code>
  </stage>
