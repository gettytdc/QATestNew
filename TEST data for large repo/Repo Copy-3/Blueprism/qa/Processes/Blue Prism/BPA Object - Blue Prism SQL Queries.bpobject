      <input type="text" name="Query" expr="[Query]" />
      <input type="text" name="Server" expr="[DB Server]" />
    </inputs>
    <onsuccess>d913109a-d0b8-4c8b-a703-28c2eeab6b52</onsuccess>
    <code><![CDATA[var connectionString = 
    string.Format(
		"database={0};data source={1};Trusted_Connection=Yes",
		Database, Server);

using(var connection = new SqlConnection(connectionString))
{
	connection.Open();
	
	var command = new SqlCommand(Query, connection);
	command.ExecuteNonQuery();
	
	connection.Close();
}]]></code>
