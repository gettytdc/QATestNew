    <font family="Segoe UI" size="10" style="Regular" color="000000" />
    <inputs>
      <input type="text" name="Database" expr="[DB Name]" />
      <input type="text" name="Query" expr="[Query]" />
      <input type="text" name="Server" expr="[DB Server]" />
    </inputs>
    <outputs>
      <output type="text" name="Output" stage="Output" />
    </outputs>
    <onsuccess>d913109a-d0b8-4c8b-a703-28c2eeab6b52</onsuccess>
    <code><![CDATA[var connectionString =
   string.Format(
        "database={0};data source={1};Trusted_Connection=Yes",
        Database, Server);

using(var connection = new SqlConnection(connectionString))
{
    connection.Open();
    
    var command = new SqlCommand(Query, connection);
    var result = command.ExecuteScalar();
    if(result == null)
        Output = string.Empty;
    else
        Output = result.ToString();
    
    connection.Close();
}]]></code>
