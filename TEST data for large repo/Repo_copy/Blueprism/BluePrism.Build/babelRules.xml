<Rules>
   <Rule name="rule1" feature="renaming" exclude="true">
      <Target>Classes,Interfaces,Structures,Enums</Target>
      <Access>Private,Protected</Access>
      <Pattern>*</Pattern>
   </Rule>
   <Rule name="rule2" feature="renaming" exclude="true" applyToMembers="true">
      <Access>Internal</Access>
      <Pattern>*</Pattern>
   </Rule>
</Rules>
